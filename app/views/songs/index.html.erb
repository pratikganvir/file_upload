<h1>Listing songs</h1>

<table>
  <thead>
    <tr>
      <th>Artist name</th>
      <th>Title</th>
      <th>Genere name</th>
      <th>Duration</th>
      <th>Price</th>
      <th>Played song</th>
      <th>Country</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.artist_name %></td>
        <td><%= song.title %></td>
        <td><%= song.genere_name %></td>
        <td><%= song.duration %></td>
        <td><%= song.price %></td>
        <td><%= song.played_song %></td>
        <td><%= song.country %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
